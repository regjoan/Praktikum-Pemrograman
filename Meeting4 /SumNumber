#include <iostream>
using namespace std;

int main() {
    int number;
    int i;
    cout << "Enter the number to calculate : ";
    cin >> number;
    
    int sum = 0;

    for(i=1;i<=number;i=i+1){
        sum = sum + i;
    }
    
    cout << "Sum of Natural Numbers = " << sum << endl;
    return 0;
}

